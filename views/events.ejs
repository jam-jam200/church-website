    <section
      class="hero-wrap hero-wrap-2 js-fullheight"
      style="background-image: url(/asset/church.jpg)"
    >
      <div class="overlay"></div>
      <div class="container">
        <div class="row no-gutters slider-text align-items-end js-fullheight">
          <div class="col-md-9 ftco-animate pb-5">
            <p class="breadcrumbs mb-2">
              <span class="mr-2"
                ><a href="/">Home <i class="fa fa-chevron-right"></i></a
              ></span>
              <span>Events <i class="fa fa-chevron-right"></i></span>
            </p>
            <h1 class="mb-0 bread">Events</h1>
          </div>
        </div>
      </div>
    </section>
    <section class="ftco-section ftco-no-pt">
      <div class="container">
        <div
          class="row justify-content-center pb-5 mb-3"
